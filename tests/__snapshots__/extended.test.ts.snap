  /**
   * The text of the review.
   * @example "This looks great."
   */
  body: string;
  body_html?: string;
  body_text?: string;
  /**
   * A commit SHA for the review.
   * @example "54bb654c9e6025347f57900a4a5c2313a96b8035"
   */
